@import "./../../components/swiper.wxss";
page{background-color:#f3f3f3;padding-bottom:20rpx}.list{padding:30rpx 30rpx 10rpx 30rpx;margin-bottom:30rpx;background-color:#fff;position:relative;border-bottom:1px solid #e6e8e7}.list .list_title{font-size:28rpx;padding-left:16rpx;border-left:6rpx solid #4fbd7b}.item_iamge{width:100%;height:300rpx}.item_content{display:flex;flex-wrap:wrap;justify-content:space-between;margin-top:30rpx}.item_content .item{width:45%;text-align:center;margin-bottom:10rpx;margin-bottom:20rpx}.item_content .expain{display:block;font-size:28rpx;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.item_foot{margin-top:6rpx}.item_foot .price{font-size:40rpx;color:#ff5b5b;text-align:left;float:left}.item_foot .count{font-size:26rpx;color:#a4a4a4;float:right;text-align:right;margin-top:10rpx}.to_top{width:90rpx;height:90rpx;border-radius:50%;border:1px solid #e6e8e7;background-color:#fff;font-size:24rpx;line-height:90rpx;text-align:center;float:right;margin-right:20rpx}.to_top_box:after{content:'';display:'block';clear:both}